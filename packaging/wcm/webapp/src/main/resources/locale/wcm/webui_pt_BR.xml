    </label>
  </UIContentBrowsePanelFolder>

  <UISelectedContentGrid>
    <label>
      <name>#{word.name}</name>
      <add>Adicionar</add>
      <delete>Excluir</delete>
    </label>
    <msg>
      <no-content-found>Nenhum conteúdo encontrado</no-content-found>
