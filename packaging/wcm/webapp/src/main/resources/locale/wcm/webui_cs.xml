      <Public>Veřejné</Public>
      <BackupCenter>Centrum zálohování</BackupCenter>
      <CategorizedDocumentsCenter>Centrum kategorizovaných dokumentů</CategorizedDocumentsCenter>
      <CollaborationCenter>Centrum spolupráce</CollaborationCenter>
      <DigitalAssetsManagementCenter>Centrum správy digitálních aktiv</DigitalAssetsManagementCenter>
      <DMSAdministration>Administrace DMS</DMSAdministration>
      <DocumentsManagementCenter>Centrum správy dokumentů</DocumentsManagementCenter>
      <LiveDocumentsCenter>Centrum live dokumentů</LiveDocumentsCenter>
      <PendingDocumentsCenter>Centrum dokumentů čekajících na zpracování</PendingDocumentsCenter>
      <RecordsManagementCenter>Centrum správy záznamů</RecordsManagementCenter>
      <SharedUsersSpace>Skupina sdílená uživateli</SharedUsersSpace>
      <Systemfiles>Systémové soubory</Systemfiles>
      <ValidationRequestsDocumentsCenter>Centrum ověření požadavků na dokumenty</ValidationRequestsDocumentsCenter>
      <ManagedSites>Správa webů</ManagedSites>
