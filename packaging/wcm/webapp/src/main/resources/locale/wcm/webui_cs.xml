      <Search>Hledat</Search>
      <Sitemap>Mapa stránek</Sitemap>
      <TwoColumnsLayout>Rozložení do dvou sloupců</TwoColumnsLayout>
      <ThreeColumnsLayout>Rozložení do tří sloupců</ThreeColumnsLayout>
      <TwoRowsLayout>Dvouřadé rozložení</TwoRowsLayout>
      <ThreeRowsLayout>Třířadé rozložení</ThreeRowsLayout>
      <TwoTabsLayout>Rozložení do dvou karet</TwoTabsLayout>
      <ThreeTabsLayout>Rozložení do tří karet</ThreeTabsLayout>
      <TwoColumnsOneRowLayout>Rozložení do dvou sloupců &amp; jednoho řádku</TwoColumnsOneRowLayout>
      <OneRowTwoColumnsLayout>Rozložení do jednoho řádku &amp; dvou sloupců</OneRowTwoColumnsLayout>
      <ThreeRowsTwoColumnsLayout>Rozložení do tří řádků &amp; dvou sloupců</ThreeRowsTwoColumnsLayout>
    </label>
  </UIWizardPageSelectLayoutForm>

  <Signin>
    <welcome>Vítejte</welcome>
    <login>Přihlášení</login>
    <logout>Odhlásit</logout>
    <language>Změnit jazyk</language>
    <register>Registrace</register>
  </Signin>
  <UIDriveForm>
<label>
<wcm-category-view>Zobrazení podle kategorií</wcm-category-view>
<wcm-view>Zobrazení obsahu WCM</wcm-view>
</label>
  </UIDriveForm>
  
  <ContentSelector>
    <title>
      <PersonalDrives>Osobní disky</PersonalDrives>
      <GeneralDrives>Obecné disky</GeneralDrives>
      <GroupDrives>Skupinové disky</GroupDrives>
