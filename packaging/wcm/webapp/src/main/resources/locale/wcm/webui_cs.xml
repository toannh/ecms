  </ContentSelector>
  
  <UICategoryNavigationTargetPathPopupWindow>
    <title>
      <UIPageSelector>Výběr stránky</UIPageSelector>
    </title>
  </UICategoryNavigationTargetPathPopupWindow>
  
  <UIDocumentInfo>
    <msg>
      <access-denied-exception>Přístup byl odepřen. Možná nemáte oprávnění ke čtení, nebo mohl být uzel odstraněn nebo změněn.</access-denied-exception>
    </msg>
  </UIDocumentInfo>
  <UIPortalApplication>
    <title>
      <abort>Přerušit</abort>
    </title>
  </UIPortalApplication>    
  
