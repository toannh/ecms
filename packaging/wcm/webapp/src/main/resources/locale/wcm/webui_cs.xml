      <TabsPageConfigs>Nastavení stránky s taby</TabsPageConfigs>
      <MixPageConfigs>Nastavení smíšené stránky</MixPageConfigs>
    </item>
  </UIDropDownPageTemp>

  <UIWizardPageSelectLayoutForm>
    <label>
      <SingleContentViewer>Jediný prohlížeč obsahu</SingleContentViewer>
